{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'todoComponent',\n  data() {\n    return {\n      todos: [\n        // { task: 'Have breakfast', delete:'(x)'},\n        // { task: 'Go to the gym', delete:'(x)'},\n        // { task: 'Study Vuejs', delete:'(x)'}\n      ],\n      todos1: [{\n        task: 'Have breakfast',\n        delete: '(x)'\n      }\n      // { task: 'Go to the gym', delete:'(x)'},\n      // { task: 'Study Vuejs', delete:'(x)'}\n      ],\n\n      task: ''\n    };\n  },\n  mounted() {\n    this.todos = JSON.parse(localStorage.getItem(\"todos\"));\n    // this.todos1 = JSON.parse(localStorage.getItem(\"todos1\"));\n  },\n\n  methods: {\n    /* eslint-disable */\n    addNewTodo: function () {\n      if (this.task != '') {\n        let todo = {\n          task: this.task,\n          delete: '(x)'\n        };\n        this.todos.push(todo);\n        localStorage.setItem(\"todos\", JSON.stringify(this.todos));\n        this.todos1 = JSON.parse(localStorage.getItem(\"todos1\"));\n        this.task = ''; // clear input\n      } else {\n        alert(\"Please enter Text\");\n      }\n    },\n    deleteTodo: function (index) {\n      this.todos.splice(index, 1);\n      this.todos1.push(this.todos[index]);\n      console.log(this.todos1);\n      localStorage.setItem(\"todos1\", JSON.stringify(this.todos1));\n      this.saveTasksToLocalStorage();\n    },\n    deleteAllTodo() {\n      if (confirm('Are You Sure?')) {\n        this.todos.length = 0;\n        this.saveTasksToLocalStorage();\n      }\n    },\n    saveTasksToLocalStorage() {\n      localStorage.setItem(\"todos\", JSON.stringify(this.todos));\n    }\n  }\n};","map":{"version":3,"mappings":";AAyBA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;QACL;QACA;QACA;MAAA,CACD;MACDC,MAAM,EAAE,CACL;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,MAAM,EAAC;MAAK;MACvC;MACA;MAAA,CACD;;MACDD,IAAI,EAAE;IACR;EACF,CAAC;EACDE,OAAO,GAAG;IACR,IAAI,CAACJ,KAAI,GAAIK,IAAI,CAAC